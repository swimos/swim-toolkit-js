{
  "projectFolder": "../..",
  "mainEntryPointFilePath": "<projectFolder>/lib/main/index.d.ts",
  "bundledPackages": [
    "@swim/model",
    "@swim/style",
    "@swim/theme",
    "@swim/view",
    "@swim/dom",
    "@swim/graphics",
    "@swim/controller",
    "@swim/ui",

    "@swim/button",
    "@swim/token",
    "@swim/table",
    "@swim/window",
    "@swim/deck",
    "@swim/ux",

    "@swim/gauge",
    "@swim/pie",
    "@swim/chart",
    "@swim/vis",

    "@swim/map",
    "@swim/mapbox",
    "@swim/leaflet",
    "@swim/googlemap",
    "@swim/esrimap",
    "@swim/maps"
  ],
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/src/main/tsconfig.json"
  },
  "apiReport": {
    "enabled": false,
    "reportFolder": "<projectFolder>/lib",
    "reportTempFolder": "<projectFolder>/lib/main",
    "reportFileName": "swim-<unscopedPackageName>.api.md"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/lib/swim-<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/dist/swim-<unscopedPackageName>.d.ts"
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/dist/tsdoc-metadata.json"
  },
  "messages": {
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      },
      "ae-incompatible-release-tags": {
        "logLevel": "warning",
        "addToApiReportFile": false
      },
      "ae-internal-missing-underscore": {
        "logLevel": "none",
        "addToApiReportFile": false
      }
    }
  }
}
